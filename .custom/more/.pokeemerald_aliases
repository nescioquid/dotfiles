# POKEEMERALD_ALIASES

alias pokeemerald_aliases='${=EDITOR} ~/.custom/more/.pokeemerald_aliases'
alias vscpokeemerald_aliases='code ~/.custom/more/.pokeemerald_aliases'
alias srcp='source ~/.custom/more/.pokeemerald_aliases && echo hey, you sourced pokeemerald things, yay!'

alias findit='f pokeemerald.sav'
alias tossit='toss pokeemerald.sav'
alias takeit='move pokeemerald.sav ~/builds/pokemon/last-save/'
alias bringit='move ~/builds/pokemon/last-save/pokeemerald.sav ~/builds/pokemon/pokeemerald/'

alias maake='make -j6'
alias makke='make clean'
alias maakke='make clean && make -j6'

alias OA='echo haha it happened again oooh aaah hahaha'

alias firstagbcc='sudo apt install build-essential binutils-arm-none-eabi git libpng-dev && git clone https://github.com/pret/agbcc && cd agbcc && ./build.sh'
alias secondagbcc='./install.sh ../pokeemerald'

alias vsc-gt='code -n ~/builds/pokemon/archive/green-thing'

alias vsc-berries='code ~/builds/pokemon/berries.md'
alias vsc-habitats='code ~/builds/pokemon/habitats.md'
alias vsc-items='code ~/builds/pokemon/items.md'
alias vsc-locations='code ~/builds/pokemon/locations.md'
alias vsc-marts='code ~/builds/pokemon/marts.md'
alias vsc-notes='code ~/builds/pokemon/notes.md'
alias vsc-pickup='code ~/builds/pokemon/pickup.md'
alias vsc-types='code ~/builds/pokemon/types.md'

# alias addporyscript='md tools/poryscript && copy ~/builds/pokemon/poryscript/poryscript-linux/font_config.json ~/builds/pokemon/pokeemerald/tools/poryscript/font_config.json && copy ~/builds/pokemon/poryscript/poryscript-linux/poryscript ~/builds/pokemon/pokeemerald/tools/poryscript/poryscript'

